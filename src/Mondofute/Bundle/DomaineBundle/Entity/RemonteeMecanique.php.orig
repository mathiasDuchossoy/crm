<?php

namespace Mondofute\Bundle\DomaineBundle\Entity;

/**
 * RemonteeMecanique
 */
class RemonteeMecanique
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $nombre;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Get nombre
     *
     * @return integer
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set nombre
     *
     * @param integer $nombre
     *
     * @return RemonteeMecanique
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;

        return $this;
    }
}
