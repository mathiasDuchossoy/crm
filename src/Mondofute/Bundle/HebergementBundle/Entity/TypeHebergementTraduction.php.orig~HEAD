<?php

namespace Mondofute\Bundle\HebergementBundle\Entity;

use Mondofute\Bundle\LangueBundle\Entity\Langue;

/**
 * TypeHebergementTraduction
 */
class TypeHebergementTraduction
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $libelle;
    /**
     * @var TypeHebergement
     */
    private $typeHebergement;
    /**
     * @var Langue
     */
    private $langue;

    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Get libelle
     *
     * @return string
     */
    public function getLibelle()
    {
        return $this->libelle;
    }

    /**
     * Set libelle
     *
     * @param string $libelle
     *
     * @return TypeHebergementTraduction
     */
    public function setLibelle($libelle)
    {
        $this->libelle = $libelle;

        return $this;
    }

    /**
     * Get typeHebergement
     *
     * @return TypeHebergement
     */
    public function getTypeHebergement()
    {
        return $this->typeHebergement;
    }

    /**
     * Set typeHebergement
     *
     * @param TypeHebergement $typeHebergement
     *
     * @return TypeHebergementTraduction
     */
    public function setTypeHebergement(
        TypeHebergement $typeHebergement = null
    ) {
        $this->typeHebergement = $typeHebergement;

        return $this;
    }

    /**
     * Get langue
     *
     * @return Langue
     */
    public function getLangue()
    {
        return $this->langue;
    }

    /**
     * Set langue
     *
     * @param Langue $langue
     *
     * @return TypeHebergementTraduction
     */
    public function setLangue(Langue $langue = null)
    {
        $this->langue = $langue;

        return $this;
    }
}
